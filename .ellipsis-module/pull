mkdir -p $HOME/.cache/ellipsis/modules/vim
vim -c 'call vice#ListAddonsIntoBuffer()' -c 'normal ggdd' -c 'w! ~/.cache/ellipsis/modules/vim/addons' -c > /dev/null 2>&1

pull $HOME/.vim/addons/vice
pull $HOME/.vim/addons/vim-addon-manager

for addon in `cat $HOME/.cache/ellipsis/modules/vim/addons`; do
    if [ -e "$addon" ]; then
        pull $addon
    fi
done
